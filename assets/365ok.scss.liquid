.panel-collapse-content{
	position: relative;
	overflow: hidden;
}
.panel-collapse-content:before {
	content: '';
	position: absolute;
	top: 200px;
	display: block;
	background: linear-gradient(rgba(255, 255, 255, 0), rgb(255, 255, 255));
	height: 200px;
	width: 100%;
	z-index: 0;
}
#product-collapse .collapsing {
	-webkit-transition: none;
	transition: none;
	display: none;
}
#product-collapse .panel-collapse-content.active{
	height: auto !important;
	overflow: initial;
}
#product-collapse .panel-collapse-content.active:before{
	display: none;
}
