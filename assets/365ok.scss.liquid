/* Bộ lọc - START */
.bo-loc{
	margin-bottom: 30px;
	position: relative;
	.filter-wrap{
		display: inline-block;
		position: relative;
	}
	.filter{
		border: 1px solid #ddd;
		padding: 10px 15px;
		border-radius: 5px;
		margin: 0 10px 0 0;
		cursor: pointer;
		transition: .5s;
		position: relative;
		display: inline-block;
		&.active{
			background: #ff0000;
			color: #fff;
			i{
				transform: rotate(180deg);
				color: #fff;
			}
			&:hover{
				background: #ff0000;
			}
		}
		&.li-checked{
			background: #ff0000;
			color: #fff;
		}
		&:hover{
			background: #ddd;
		}
		i {
			margin-left: 8px;
			transition: .5s;
		}

	}
	.filter-item{
		position: absolute;
		top: 45px;
		left: 0;
		background: #fff;
		padding: 25px;
		z-index: 2;
		color: #464646;
		pointer-events: none;
		opacity: 0;
		transition: .5s;
		box-shadow: 0 9px 25px 0 rgba(0,0,0,.05), 0 3px 12px 0 rgba(0,0,0,.1);
		column-count: 2;
		column-gap: 20px;
		height: 300px;
		overflow: auto;
		margin-top: 10px;
		&.active{
			pointer-events: initial;
			opacity: 1;
		}
		li{	
			position: relative;
			white-space: nowrap;
			padding: 0 0 0 26px;
			margin-bottom: 12px;
			line-height: 22px;
			cursor: pointer;
			&:before{
				content: '';
				width: 22px;
				height: 22px;
				position: absolute;
				left: 0;
				background: url({{ 'icon-check.png' | asset_url }}) no-repeat center center/cover;
				background-position: 0 1px;
				top: 50%;
				transform: translateY(-50%);
			}
			&.active{
				color: #ff0000;
				&:before{
					background-position: -44px 0;
				}
			}
		}
		.clear{
			position: absolute;
			bottom: 5px;
			left: 25px;
			cursor: pointer;
			&:hover{
				text-decoration: underline;
			}
		}
		.done{
			position: absolute;
			right: 25px;
			bottom: 5px;
			color: #ff0000;
			cursor: pointer;
			&:hover{
				text-decoration: underline;
			}
		}
	}
	.filter-item-mau-sac{
		li{
			display: -webkit-box;
			display: -moz-box;
			display: -ms-flexbox;
			display: -moz-flex;
			display: -webkit-flex;
			display: flex;
			align-items: center;
			.color{
				width: 18px;
				height: 18px;
				display: inline-block;
				border-radius: 50%;
				padding: 2px;
				border: 1px solid #ebebeb;
				margin: 0 5px 0 10px;
				position: relative;
				&:before{
					content: '';
					background: #fff;
					width: calc(100% - 4px);
					height: calc(100% - 4px);
					display: inline-block;
					border-radius: 50%;
					position: absolute;
					top: 2px;
					left: 2px;
					padding: 2px;
				}
			}
		}
	}
	.filter-item-gia{
		column-count: initial;
		column-gap: initial;
		height: auto;
		.ui-slider-horizontal .ui-slider-range{
			height: 5px !important;
			background: #ff0000 !important;
		}
		.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default, .ui-button, html .ui-button.ui-state-disabled:hover, html .ui-button.ui-state-disabled:active{
			border-radius: 50%;

			top: 2px;
			transform: translateY(-50%);
			&:focus{
				border-radius: 50%;

				top: 2px;
				transform: translateY(-50%);
			}
		}
		#slider-range{
			margin-top: 12px;
			height: 7px;
			margin-bottom: 5px;
		}
		#number-range{
			display: -webkit-box;
			display: -moz-box;
			display: -ms-flexbox;
			display: -moz-flex;
			display: -webkit-flex;
			display: flex;
			justify-content: space-between;
			width: 270px;

			input{
				width: max-content;
				height: 40px;
				border-radius: 5px;
				border: 1px solid #ababab;
				text-align: center;
				padding-right: 25px;
				width: 125px;
			}
			/* Chrome, Safari, Edge, Opera */
			input::-webkit-outer-spin-button,
			input::-webkit-inner-spin-button {
				-webkit-appearance: none;
				margin: 0;
			}

			/* Firefox */
			input[type=number] {
				-moz-appearance: textfield;
			}
			.range{
				position: relative;
				span{
					position: absolute;
					top: 50%;
					right: 5%;
					transform: translateY(-50%);
					font-weight: bold;
				}
			}
		}
	}
	.sort-by{
		position: absolute;
		right: 0;
		top: 50%;
		transform: translateY(-50%);
		select{
			border-radius: 5px;
			text-transform: none;
			border: 1px solid #ddd;
			outline: none;
			padding: .65em .75em .55em;
			background-color: #fff;
		}
	}
}
/* Bộ lọc - END */



