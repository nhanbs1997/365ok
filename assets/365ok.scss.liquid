#product-collapse{
	display: inline-block;
	width: 100%;
	img{
		width: 100%;
		object-fit: contain;
		height: auto;	
	}
	.panel{
		border: none;
		border-radius: 0;
		box-shadow: none;
		margin: 0;
		.panel-heading{
			padding: 0;
			cursor: pointer;
			.collapse-title{
				position: relative;
				font-weight: bold;
				height: 40px;
				line-height: 40px;
				padding-left: 10px;
				-webkit-box-shadow: 0 1px 1px rgba(0,0,0,.05);
				box-shadow: 0 1px 1px rgba(0,0,0,.05);
				border-bottom: 1px solid #ccc;
				span{
					position: absolute;
					right: 15px;
					top: 50%;
					transform: translateY(-50%);


				}
				i{
					transform: rotate(180deg);
					font-weight: bold;
					transition: .5s;
				}
			}
			.collapse-title.collapsed{
				i{
					transform: rotate(0deg);
				}
			}
		}
		.panel-collapse{
			padding: 10px;
			border-bottom: 1px solid #ccc;
			height: auto !important;
		}
	}
	#product-detail table, #product-detail1 table {
		border: 1px solid #e1e1e1;
		border-top: none; 
		width: 100% !important;
	}
	#product-detail table td, #product-detail1 table td{
		border: 1px solid #e1e1e1;
		padding: 10px !important;
		width: 50% !important;
		height: auto !important;
	}
	#product-detail table tr, #product-detail1 table tr{
		height: auto !important;
	}
	#product-detail table td:first-child, #product-detail1 table td:first-child{
		font-weight: bold;
		width: max-content;
		margin: auto;
		padding: 10px;
		box-sizing: border-box;
		color: #303030;
		background: #f4f4f4;
		border-right: 1px solid #e1e1e1; 
	}
}
.collection-cate .lists-cate{
	display: -webkit-box;
