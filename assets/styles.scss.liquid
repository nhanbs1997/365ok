/*** fixed bottom xs **/

.fixed-bottom {
	background-color: #fff;
	width: 100%;
	float: left;
	position: fixed;
	bottom: 0;
	left: 0;
	padding: 10px;
	border-top: 1px solid #999;
	z-index: 999;
	.fixed-product-title {
		margin-bottom: 5px;
		font-weight: bold;
		font-size: 14px;
		text-align: center;
	}
	.fix-active {
		color: #fff;
		height: 30px;
		line-height: 30px;
		padding: 0 20px;
		.fixed-price {
			background: #333;
			text-align: center;
		}
		.fixed-btn {
			background: #000;
			button {
				border: none;
				width: 100%;
				background: #000;
			}
		}
	}
}

@media screen and (min-width: 768px) {
	#collection .products-grid .col-sm-4:nth-child(3n + 1) {
		clear: both;
	}
}

.variant-image {
	width: 50px;
	height: 50px;
	object-fit: contain;
	border: 1px solid transparent;
	padding: 2px;
	border-radius: 5px;
	cursor: pointer;
}
.variant-image.active {
	border: 1px solid #ff0000;
}

#quick-view-modal ~ img {
	display: none !important;
}
.index-product {
	background: #fafafa;
	.index-title {
		margin-top: 25px;
	}
	.owl-prev {
		position: absolute;
		top: 50%;
		left: 0;
		transform: translateY(-50%);
		background: #fff !important;
		width: 50px;
		height: 50px;
		border-radius: 50% !important;
		font-size: 33px !important;
		box-shadow: 0 4px 16px 0 rgba(0, 0, 0, 0.1), 0 1px 6px 0 rgba(0, 0, 0, 0.12);
		&:hover {
			i {
				color: #000;
			}
		}
	}
	.owl-next {
		position: absolute;
		top: 50%;
		right: 0;
		transform: translateY(-50%);
		background: #fff !important;
		width: 50px;
		height: 50px;
		border-radius: 50% !important;
		font-size: 33px !important;
		box-shadow: 0 4px 16px 0 rgba(0, 0, 0, 0.1), 0 1px 6px 0 rgba(0, 0, 0, 0.12);
		&:hover {
			i {
				color: #000;
			}
		}
	}
}
#product-description ul {
	list-style: initial !important;
}
.index-cate {
	position: relative;
}
#countdown-txt {
	position: fixed;
	right: 0;
	top: 50%;
	transform: translateY(-50%);
	z-index: 1000;
	.fa-close {
		font-size: 20px;
		display: block;
		cursor: pointer;
		margin-bottom: 5px;
	}
	.txt {
		text-align: center;
		color: #fff;
		background: #ff640b;
		height: 40px;
		line-height: 40px;
		font-size: 15px;
		font-weight: bold;
		position: relative;
		padding-left: 39px;
		&:hover {
			opacity: 0.8;
		}
		i {
			position: absolute;
			left: 0;
			top: 50%;
			transform: translateY(-50%);
			font-size: 20px;
			background: #ff8641;
			height: 39px;
			line-height: 39px;
			width: 39px;
		}
	}
	.countdown {
		display: -webkit-box;
		display: -moz-box;
		display: -ms-flexbox;
		display: -moz-flex;
		display: -webkit-flex;
		display: flex;
		color: #ff640b;
	}
	span {
		text-align: center;
		font-weight: bold;
		margin: 0 5px;
	}
	p {
		color: #848484;
		font-size: 12px;
		font-weight: normal;
	}
}

@font-face {
	font-family: "themify";
	src: url("{{ "themify.eot" | asset_url }}");
	src: url("{{ "themify.eot?#iefix" | asset_url }}") format("embedded-opentype"),
		url("{{ "themify.woff" | asset_url }}") format("woff"),
		url("{{ "themify.ttf" | asset_url }}") format("truetype"), url("{{ "themify.svg" | asset_url }}") format("svg");
	font-weight: normal;
	font-style: normal;
}

[class^="ti-"],
[class*=" ti-"] {
	font-family: "themify";
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.ti-bolt:before {
	content: "\e652";
}
.ti-bolt-alt:before {
	content: "\e653";
}
